{
    "name": "viitortest/contactable",
    "description": "Provide the contacts to store multiple contacts at one place",
    "license": "MIT",
    "authors": [{
        "name": "Dhaval Joshi",
        "email": "dhaval.joshi@viitor.cloud"
    }],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Viitortest\\Contactable\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viitortest\\Contactable\\ContactServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}